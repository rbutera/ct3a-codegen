{
  "name": "ct3a-codegen",
  "version": "0.0.1",
  "description": "Code generation tool for NextJS / create-t3-app projects powered by Plop",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rsbuild build && node scripts/add-shebang.js",
    "postbuild": "cp -R templates dist/",
    "create": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "plop",
    "codegen",
    "nextjs",
    "create-t3-app",
    "t3",
    "trpc",
    "react"
  ],
  "author": "Rai Butera <rai@rbutera.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "find-up": "^7.0.0",
    "fs-extra": "^11.2.0",
    "plop": "^4.0.1"
  },
  "devDependencies": {
    "@rsbuild/core": "1.0.0-alpha.3",
    "@rspack/cli": "1.0.0-alpha.1",
    "@rspack/core": "1.0.0-alpha.1"
  }
}
